<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


<bean name="address1" class="com.springframework.bean.Address">
<property name="city" value="Ahemdabad"/>
<property name="state" value="Gujarat"/>
<property name="country" value="India"/>
</bean>

<bean name="address2" class="com.springframework.bean.Address">
<property name="city" value="Mumbai"/>
<property name="state" value="Maharastra"/>
<property name="country" value="India"/>

</bean>
<bean name="address3" class="com.springframework.bean.Address">
<property name="city" value="Delhi"/>
<property name="state" value="Delhi"/>
<property name="country" value="India"/>
</bean>
<bean name="address4" class="com.springframework.bean.Address">
<property name="city" value="Jaipur"/>
<property name="state" value="rajasthan"/>
<property name="country" value="India"/>
</bean>





<bean name="employee" class="com.springframework.bean.Employee">
<property name="id" value="101"/>
<property name="name" value="Aniket Gajjar"></property>
<property name="address">
<list>
<ref bean="address1"/>
<ref bean="address2"/>
<ref bean="address3"/>
<ref bean="address4"/>
</list>
</property></bean>
</beans>
