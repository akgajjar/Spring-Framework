<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


<bean id="question" class="com.springframework.bean.Question">  
<constructor-arg value="11"></constructor-arg>  
<constructor-arg value="What is Java?"></constructor-arg>  
<constructor-arg>  
<map>  
<entry value="Java is a Programming Language"  key="Ajay Kumar"></entry>  
<entry value="Java is a Platform" key="John Smith"></entry>  
<entry value="Java is an Island" key="Raj Kumar"></entry>  
</map>  
</constructor-arg>  
</bean>  
</beans>
